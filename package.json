{
  "name": "glints-aries",
  "version": "4.0.0-beta.13",
  "description": "Glints ui-kit for frontend",
  "main": "dist/index.js",
  "scripts": {
    "assets:build": "webpack && cp -R assets/fonts/ dist/public/assets/fonts/",
    "build": "rm -rf dist && babel -d dist --copy-files --extensions .ts,.tsx src && tsc -d && npm run assets:build",
    "prepublishOnly": "npm run build",
    "storybook": "npm run assets:build && start-storybook -p 9001 -c storybook-config -s dist/public",
    "storybook:build": "rm -rf .out && npm run assets:build && build-storybook -c storybook-config -o .out -s dist/public",
    "test:update": "jest -u",
    "test": "jest",
    "lint:css": "stylelint './src/**/*.{ts,tsx}'"
  },
  "author": "Glints",
  "license": "MIT",
  "private": false,
  "keywords": [
    "react",
    "glints-aries",
    "glints-uikit"
  ],
  "bugs": {
    "url": "https://github.com/glints-dev/glints-aries/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
